<Categories>
	<questions>
		<quest-item id="1" question="Jak lubisz spędzać wolny czas?">
			<answer-item id="1" goto-quest-id="2">a) W wolnym czasie chcę poprostu odpocząć i zostać w domu.</answer-item>
			<answer-item id="2" goto-quest-id="3">b) Lubię spędzać czas aktywnie.</answer-item>
			<answer-item id="3" goto-quest-id="4">c) Nie mam czasu wolnego, mam dużo obowiązków domowych.</answer-item>
		</quest-item>
		<quest-item id="2" question="Co najbardziej lubisz robić w domu?">
			<answer-item id="4" goto-quest-id="0">a) Najbardzie lubię oglądać telewizję.</answer-item>
			<answer-item id="5" goto-quest-id="0">b) Lubię grać w gry komputerowe.</answer-item>
			<answer-item id="6" goto-quest-id="0">c) Lubię słuchać muzuki.</answer-item>
		</quest-item>
		<quest-item id="3" question="Jaki rodzaj aktywności jest dla ciebie najlepszy?">
			<answer-item id="7" goto-quest-id="5">a) Najbardziej lubię biegać.</answer-item>
			<answer-item id="8" goto-quest-id="6">b) Bardzo często jeżdżę na rowerze.</answer-item>
			<answer-item id="9" goto-quest-id="7">c) Lubię chodzić na siłownie.</answer-item>
		</quest-item>
		<quest-item id="4" question="Jakie prace domowe wykonujesz najczęściej?">
			<answer-item id="10" goto-quest-id="0">a) Pracuję w ogrodzie.</answer-item>
			<answer-item id="11" goto-quest-id="0">b) Naprawiam lub buduję coś nowego.</answer-item>
			<answer-item id="12" goto-quest-id="0">c) Sprzątam.</answer-item>
		</quest-item>
		<quest-item id="5" question="Jakie akcesoria/sprzęt do biegania jest dla Ciebie najważniejszy?">
			<answer-item id="16" goto-quest-id="0">a) Dobre buty.</answer-item>
			<answer-item id="17" goto-quest-id="0">b) Odzież.</answer-item>
			<answer-item id="18" goto-quest-id="0">c) Odżywki/suplementy.</answer-item>
		</quest-item>
		<quest-item id="6" question="Na jakim rowerze lubisz jeździć najbardziej?">
			<answer-item id="13" goto-quest-id="0">a) Na rowerze górskim.</answer-item>
			<answer-item id="14" goto-quest-id="0">b) Na rowerze szosowym.</answer-item>
			<answer-item id="15" goto-quest-id="0">c) Na rowerze turystycznym.</answer-item>
		</quest-item>
		<quest-item id="7" question="Jakie ćwiczenia preferujesz?">
			<answer-item id="19" goto-quest-id="0">a) Fitness.</answer-item>
			<answer-item id="20" goto-quest-id="0">b) Cwiczenia siłowe.</answer-item>
		</quest-item>
	</questions>

